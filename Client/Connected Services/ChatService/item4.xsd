<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/Server.Clases.Base" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/Server.Clases.Base" xmlns:xs="http://www.w3.org/2001/XMLSchema">
